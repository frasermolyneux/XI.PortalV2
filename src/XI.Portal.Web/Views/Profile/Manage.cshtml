@{
    ViewData["Title"] = "Manage Profile";
}

<div class="text-center">
    <h1 class="display-4">Profile</h1>
</div>

<row class="row">
    <table class="table">
        <thead>
        <tr>
            <td>Type</td>
            <td>Value</td>
        </tr>
        </thead>
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
</row>