{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Debug",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Event;Trace"
      }
    },
    "enableLiveMetrics": false,
    "enableDependencyTracking": false,
    "enablePerformanceCountersCollection": false
  }
}